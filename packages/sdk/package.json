{
  "name": "@benrobo/modelkit",
  "pricing": false,
  "version": "0.0.3",
  "publishConfig": {
    "access": "public"
  },
  "description": "Type-safe, zero-downtime AI model configuration management with runtime overrides",
  "keywords": [
    "ai",
    "llm",
    "model-config",
    "configuration",
    "redis",
    "typescript",
    "runtime-override",
    "zero-downtime"
  ],
  "homepage": "https://github.com/benrobo/modelkit#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/benrobo/modelkit.git",
    "directory": "packages/sdk"
  },
  "bugs": {
    "url": "https://github.com/benrobo/modelkit/issues"
  },
  "license": "MIT",
  "author": "benrobo",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./hono": {
      "import": "./dist/router/hono.js",
      "types": "./dist/router/hono.d.ts"
    },
    "./express": {
      "import": "./dist/router/express.js",
      "types": "./dist/router/express.d.ts"
    }
  },
  "bin": {
    "modelkit-generate": "./dist/generate.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/router/hono.ts src/router/express.ts --format esm --dts && bun build scripts/generate.ts --outdir dist --target node",
    "dev": "tsup src/index.ts src/router/hono.ts src/router/express.ts --format esm --dts --watch",
    "type-check": "tsc --noEmit",
    "generate:models": "bun scripts/generate-models.ts",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "ioredis": "^5.4.1",
    "node-cache": "^5.1.2"
  },
  "peerDependencies": {
    "express": "^4.0.0 || ^5.0.0",
    "hono": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "hono": {
      "optional": true
    }
  },
  "devDependencies": {
    "@openrouter/ai-sdk-provider": "^2.2.3",
    "@types/express": "^5.0.6",
    "@types/node": "^22.13.13",
    "express": "^5.2.1",
    "hono": "^4.11.9",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
